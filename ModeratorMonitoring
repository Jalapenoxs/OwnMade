-- Send notifications for existing players
for _, userId in ipairs(Celebs) do
    notifyPlayer(userId, "Celeb")
end

for _, userId in ipairs(Influencer) do
    notifyPlayer(userId, "Influencer")
end

for _, userId in ipairs(Verified) do
    notifyPlayer(userId, "Verified")
end

-- Set up join notifications
for _, userId in ipairs(Celebs) do
    setupPlayerJoinNotification(userId, "Celeb")
end

for _, userId in ipairs(Influencer) do
    setupPlayerJoinNotification(userId, "Influencer")
end

for _, userId in ipairs(Verified) do
    setupPlayerJoinNotification(userId, "Verified")
end

