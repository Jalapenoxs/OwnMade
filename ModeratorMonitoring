local StarterGui = game:GetService("StarterGui")
local Players = game:GetService("Players")

-- Generic notification function
local function notifyPlayer(userId, title)
    local player = Players:GetPlayerByUserId(userId)
    if player then
        StarterGui:SetCore("SendNotification", {
            Title = title,
            Text = "Username: " .. player.Name,
            Duration = 6
        })
    end
end

-- Setup PlayerAdded listener for a specific userId and title
local function setupPlayerJoinNotification(userId, title)
    Players.PlayerAdded:Connect(function(plr)
        if plr.UserId == userId then
            notifyPlayer(userId, title)
        end
    end)
end

